local kavoUi = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()
local window = kavoUi.CreateLib("Granny","BloodTheme")

---Tabs

local Tab1 = window:NewTab("Main")
local Tab1Section = Tab1:NewSection("Main")
local Tab2 = window:NewTab("Player")
local Tab2Section = Tab2:NewSection("Player (turn on anti kick before)")
 
---Buttons

Tab1Section:NewButton("Esp","Esp",function()
loadstring(game:HttpGet('https://raw.githubusercontent.com/Frindow/RobloxScripts/main/Granny.lua'))()
end)

Tab1Section:NewButton("Inf yield (must type anti kick)","turn anti kick on or player wont work",function()
loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)

Tab2Section:NewSlider("Speed", "Sussy Speed", 20, 6, function(v)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
 end)

Tab2Section:NewSlider("Fov", "Sussy Fov", 120, 16, function(v)
Workspace.CurrentCamera.FieldOfView = v
end)

Tab2Section:NewButton("Fly Gui V3","lets fly",function()
loadstring(game:HttpGet('https://pastebin.com/raw/VTQJmBuD'))()
end)

Tab2Section:NewButton("Noclip","bet.",function()
loadstring(game:HttpGet('https://pastebin.com/raw/pKV2Ys4E'))()
end)
